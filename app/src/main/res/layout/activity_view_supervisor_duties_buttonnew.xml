<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#84D6CE"
    android:orientation="vertical"
    tools:context="com.example.ViewSupervisorDutiesButtonnew">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/purple_200"
        app:title="      Supervisor Duties"
        />

    <ImageView
        android:id="@+id/rback"
        android:layout_marginTop="-50dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <EditText
        android:layout_margin="8dp"
        android:id="@+id/editsuperid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Supervisor id"/>
    <Button
        android:layout_margin="8dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="find"

        android:id="@+id/find"/>
   <!-- <SearchView
        android:id="@+id/searchView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/rback"
        />-->
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/supervisorrecycle"
        android:layout_width="match_parent"
        tools:listitem="@layout/viewsupervisiorlayoutfileduties"
        android:layout_height="wrap_content"/>


</LinearLayout>
