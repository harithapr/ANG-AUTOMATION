<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homepa"
    tools:context="com.example.anganteacherhomepage">

    <Button
        android:id="@+id/dailytracking"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/boxoutlinebutton"
        android:gravity=""
        android:text="Daily Tracking"
        android:textColor="@color/black"
        android:textSize="20dp" />
    <Button
        android:id="@+id/beneficiaries"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/boxoutlinebutton"
        android:text="Beneficiaries"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:gravity=""
        />

    <LinearLayout
        android:id="@+id/healthinspectorlnr"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <ImageView
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:background="@drawable/stud"
            android:layout_marginTop="500dp"
            android:layout_marginLeft="50dp"/>

        <Button
            android:id="@+id/studreg"
            android:layout_width="124dp"
            android:layout_height="40dp"
            android:layout_marginLeft="50dp"
            android:background="#FF5722"
            android:text="Student Reg"
            android:textSize="10dp" />



    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:background="@drawable/member"
            android:layout_marginTop="500dp"
            android:layout_marginLeft="200dp"/>

        <Button
            android:id="@+id/memberdetails"
            android:layout_width="124dp"
            android:layout_height="40dp"
            android:layout_marginLeft="200dp"
            android:background="#FF5722"
            android:text="Member Details"
            android:textSize="10dp" />


    </LinearLayout>


</FrameLayout>