<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#E9EDE5E5"
    tools:context=".cdporeports">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#28022E"
            app:title="Reports"
            />

    </com.google.android.material.appbar.AppBarLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/frame_layout2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="50dp"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >



        <Button
            android:layout_marginTop="10dp"
            android:id="@+id/panchayathreports"
            android:layout_width="211dp"
            android:layout_height="149dp"
            android:layout_marginLeft="50dp"
            android:background="#BB64CA"
            android:text="PANCHAYATH REPORTS"
            android:textSize="14dp" />


        <Button
            android:layout_marginTop="10dp"
        android:id="@+id/supervisorreports"
        android:layout_width="211dp"
        android:layout_height="149dp"
        android:layout_marginLeft="50dp"
        android:background="#BA66C6"
        android:text="SUPERVISORS REPORTS"
        android:textSize="14dp" />




        <Button
            android:layout_marginTop="10dp"
            android:id="@+id/teacherreports"
            android:layout_width="211dp"
            android:layout_height="149dp"
            android:layout_marginLeft="50dp"
            android:background="#C16CCF"
            android:text="TEACHER REPORTS"
            android:textSize="14dp" />
    </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>

